    "@douyinfe/semi-ui": "^2.86.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tiptap/core": "^3.2.1",
    "@tiptap/extension-bubble-menu": "^3.2.1",
    "@tiptap/extension-character-count": "^3.2.1",
    "@tiptap/extension-code-block": "^3.2.1",
    "@tiptap/extension-code-block-lowlight": "^3.2.1",
    "@tiptap/extension-heading": "^3.2.1",
    "@tiptap/extension-highlight": "^3.2.1",
    "@tiptap/extension-image": "^3.2.1",
    "@tiptap/extension-link": "^3.2.1",
    "@tiptap/extension-placeholder": "^3.2.1",
    "@tiptap/extension-table": "^3.2.1",
    "@tiptap/extension-table-cell": "^3.2.1",
    "@tiptap/extension-table-header": "^3.2.1",
    "@tiptap/extension-table-row": "^3.2.1",
    "@tiptap/extension-text-align": "^3.2.1",
    "@tiptap/extension-text-style": "^3.2.1",
    "@tiptap/extension-typography": "^3.2.1",
    "@tiptap/extension-underline": "^3.2.1",
    "@tiptap/pm": "^3.2.1",
    "@tiptap/react": "^3.2.1",
    "@tiptap/starter-kit": "^3.2.1",
    "@tiptap/suggestion": "^3.2.1",
    "@xyflow/react": "^12.6.4",
    "ai": "^5.0.10",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.2.0",
    "docx": "^9.5.1",
    "dropbox": "^10.34.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.4",
    "echarts-wordcloud": "^2.1.0",
    "electron-updater": "^6.6.7",
    "googleapis": "^159.0.0",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.13",
    "katex": "^0.16.22",
    "lodash.throttle": "^4.1.1",
    "lowlight": "^3.3.0",
    "lru-cache": "^11.1.0",
    "md-to-pdf": "^5.2.4",
    "openai": "^5.12.2",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
